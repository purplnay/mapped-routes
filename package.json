{
  "name": "mapped-routes",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/purplnay/mapped-routes.git",
  "author": "Nay <nay@purpltag.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --rootDir ./src --outDir ./dist -d"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "express": "^4.17.1"
  },
  "dependencies": {
    "@types/express": "^4.17.9"
  }
}
