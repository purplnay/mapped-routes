{
  "name": "mapped-routes",
  "version": "1.0.8",
  "main": "dist/index.js",
  "repository": "https://github.com/purplnay/mapped-routes.git",
  "author": "Nay <nay@purpltag.com>",
  "license": "MIT",
  "homepage": "https://github.com/purplnay/mapped-routes/",
  "description": "Next.js style routes for Express, with methods support.",
  "keywords": [
    "dynamic-routes",
    "nextjs",
    "typescript",
    "express",
    "dynamic-routing",
    "router"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --rootDir ./src --outDir ./dist -d",
    "docs": "typedoc --out docs src",
    "test": "ts-mocha test/*.test.ts",
    "prepare": "npm run build"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "peerDependencies": {
    "express": "^4.17.14"
  },
  "dependencies": {
    "fs-readdir-recursive": "^1.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.14",
    "@types/fs-readdir-recursive": "^1.1.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "express": "^4.18.2",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-mocha": "^10.0.0",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  }
}
